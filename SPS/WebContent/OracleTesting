CREATE TABLE SPESTBIP (

		DEPT_ID CHAR(6) NOT NULL,
		BILL_SETTLEMENT_NO VARCHAR2(10) NOT NULL,
		BILL_REFERENCE_NO VARCHAR2(10),
		PROJECT_NO CHAR(20) NOT NULL,
		BILL_DATE DATE,
		TOTAL_BILL_AMT NUMBER(10 , 2),
		RETENTION_AMT NUMBER(10 , 2),
		WITHHOLD_AMT NUMBER(10 , 2),
		PAY_AMT NUMBER(10 , 2)
	);

CREATE UNIQUE INDEX PK_SPESTBIP ON SPESTBIP (DEPT_ID ASC, PROJECT_NO ASC, BILL_SETTLEMENT_NO ASC);

CREATE UNIQUE INDEX SYS_C0024969 ON SPESTBIP (PROJECT_NO ASC);

ALTER TABLE SPESTBIP ADD CONSTRAINT PK_SPESTBIP PRIMARY KEY (DEPT_ID, PROJECT_NO, BILL_SETTLEMENT_NO);

ALTER TABLE SPESTBIP ADD CONSTRAINT FK_SPESTBIP_D FOREIGN KEY (DEPT_ID) REFERENCES GLDEPTM (DEPT_ID) ON DELETE RESTRICT ON UPDATE CASCADE;	

