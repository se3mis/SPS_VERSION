drop table SPESTSET
create table SPESTSET(
	APPLICATION_NO VARCHAR2(20),
	DEPT_ID CHAR(6),
	TOTAL_LENGTH VARCHAR2(10),
	SERVICE_WIRE_TYPE  VARCHAR2(10),
	SERVICE_LENGTH  VARCHAR2(10),
	SERVICE_WIRE_DESCR  VARCHAR2(50),
	BARECON_TYPE  VARCHAR2(10),
	BARECON_LENGTH  NUMBER (6,2),
	BARECON_DESCR VARCHAR2(50),
	CONVERSION1_QTY  NUMBER (6,2),
	CONVERSION1_DESCR  VARCHAR2(50),
	CONVERSION2_QTY  NUMBER (6,2),
	CONVERSION2_DESCR  VARCHAR2(50),
	NO_OF_SPANS  NUMBER (3),
