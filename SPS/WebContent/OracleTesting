ALTER TABLE SPESTCND ADD CONSTRAINT FK_SPESTCND FOREIGN KEY (DEPT_ID) REFERENCES GLDEPTM (DEPT_ID) ON DELETE RESTRICT ON UPDATE CASCADE;


CREATE TABLE SPESTCNT (
		CONTRACTOR_ID VARCHAR2(4) NOT NULL,
		CONTRACTOR_NAME VARCHAR2(50),
		CONTRACTOR_ADDRESS VARCHAR2(100),
		DEPT_ID CHAR(6) NOT NULL,
		JOB_IN_HAND NUMBER(3 , 0),
		TOTAL_AMOUNT NUMBER(10 , 2),
		BOND_NO VARCHAR2(20),
		START_DATE DATE,
		PERFORMANCE_AMOUNT NUMBER(10 , 2),
		END_DATE DATE,
		CODE VARCHAR2(4),
		BOND_AMOUNT NUMBER(10 , 2),
		TENDER_AMOUNT NUMBER(10 , 2),
		SP_ADD_SPAN NUMBER(10 , 2),
		TP_ADD_SPAN NUMBER(10 , 2)
	);

CREATE UNIQUE INDEX PK_SPESTCNT ON SPESTCNT (CONTRACTOR_ID ASC, DEPT_ID ASC);

ALTER TABLE SPESTCNT ADD CONSTRAINT PK_SPESTCNT PRIMARY KEY (CONTRACTOR_ID, DEPT_ID);

ALTER TABLE SPESTCNT ADD CONSTRAINT FK_SPESTCNT FOREIGN KEY (DEPT_ID) REFERENCES GLDEPTM (DEPT_ID) ON DELETE RESTRICT ON UPDATE CASCADE;


//drop TABLE SPESTEDY
CREATE TABLE SPESTEDY (
		APPOINTMENT_ID VARCHAR2(11) NOT NULL,
		DEPT_ID CHAR(6) NOT NULL,
		APPOINMENT_TYPE VARCHAR2(10),
		REFERENCE_NO VARCHAR2(20),
		APPOINTMENT_DATE DATE,
		TIME_SESSION VARCHAR2(10),
		SUBURB VARCHAR2(50),
		STATUS CHAR(1),
		ALLOCATED_BY VARCHAR2(10),
		ALLOCATED_DATE DATE,
		ALLOCATED_TIME VARCHAR2(11),
		ALLOCATED_TO VARCHAR2(10),
		DESCRIPTION VARCHAR2(100)
		
	);

CREATE UNIQUE INDEX PK_SPESTEDY ON SPESTEDY (DEPT_ID ASC, APPOINTMENT_ID ASC);

CREATE UNIQUE INDEX UC_SPESTEDY ON SPESTEDY (DEPT_ID ASC, REFERENCE_NO ASC, APPOINMENT_TYPE ASC, STATUS ASC);

ALTER TABLE SPESTEDY ADD CONSTRAINT PK_SPESTEDY PRIMARY KEY (DEPT_ID, APPOINTMENT_ID);

ALTER TABLE SPESTEDY ADD CONSTRAINT FK_SPESTEDY FOREIGN KEY (DEPT_ID) REFERENCES GLDEPTM (DEPT_ID) ON DELETE RESTRICT ON UPDATE CASCADE;


CREATE TABLE SPESTJAD (
		DEPT_ID CHAR(6) NOT NULL,
		JOB_AMOUNT_DETAIL_ID VARCHAR2(4) NOT NULL,
		JOB_CATEGORY_ID VARCHAR2(4),
		JOB_CATEGORY VARCHAR2(100),
		JOB_DESCRIPTION VARCHAR2(200),
		UNIT_PRICE NUMBER(7 , 2)
	);

CREATE UNIQUE INDEX PK_JOB_AMOUNT_DETAILS ON SPESTJAD (DEPT_ID ASC, JOB_AMOUNT_DETAIL_ID ASC);

ALTER TABLE SPESTJAD ADD CONSTRAINT PK_JOB_AMOUNT_DETAILS PRIMARY KEY (DEPT_ID, JOB_AMOUNT_DETAIL_ID);

ALTER TABLE SPESTJAD ADD CONSTRAINT FK_JOB_AMOUNT_DETAILS FOREIGN KEY (DEPT_ID) REFERENCES GLDEPTM (DEPT_ID)

	ON DELETE RESTRICT

	ON UPDATE CASCADE;
	
	
CREATE TABLE SPESTMTM (
		PHASE NUMBER(1 , 0) NOT NULL,
		CONNECTION_TYPE NUMBER(2 , 0) NOT NULL,
		FROM_LENGTH NUMBER(4 , 0) NOT NULL,
		TO_LENGTH NUMBER(4 , 0) NOT NULL,
		WIRING_TYPE CHAR(2) NOT NULL,
		MAT_CD CHAR(20) NOT NULL,
		DEPT_ID CHAR(6) NOT NULL,
		MAT_QTY NUMBER(6 , 2),
		UPD_USER VARCHAR2(8),
		UPD_DATE DATE
		
		

	);

CREATE UNIQUE INDEX PK_SPESTMTM ON SPESTMTM (PHASE ASC, CONNECTION_TYPE ASC, FROM_LENGTH ASC, TO_LENGTH ASC, WIRING_TYPE ASC, MAT_CD ASC, DEPT_ID ASC);

ALTER TABLE SPESTMTM ADD CONSTRAINT PK_SPESTMTM PRIMARY KEY (PHASE, CONNECTION_TYPE, FROM_LENGTH, TO_LENGTH, WIRING_TYPE, MAT_CD, DEPT_ID);

ALTER TABLE SPESTMTM ADD CONSTRAINT FK_SPESTMTM FOREIGN KEY (MAT_CD) REFERENCES INMATM (MAT_CD) ON DELETE RESTRICT ON UPDATE CASCADE;

ALTER TABLE SPESTMTM ADD CONSTRAINT FK_SPESTMTM_D FOREIGN KEY (DEPT_ID) REFERENCES GLDEPTM (DEPT_ID) ON DELETE RESTRICT ON UPDATE CASCADE;

ALTER TABLE SPESTMTM ADD CONSTRAINT PK_SPESTMTM PRIMARY KEY (PHASE, CONNECTION_TYPE, WIRING_TYPE, MAT_CD, DEPT_ID);

alter table SPESTMTM drop (FROM_LENGTH)
alter table SPESTMTM drop (TO_LENGTH)

