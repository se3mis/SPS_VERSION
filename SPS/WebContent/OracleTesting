CREATE TABLE SPAPPLON (	
	ID_NO VARCHAR2(10) NOT NULL,
	APPLICATION_NO VARCHAR2(21) NOT NULL,	
	DEPT_ID CHAR(6) NOT NULL,
	LOAN_REFERENCE VARCHAR2(10),
	LOAN_AMOUNT NUMBER(10 , 2),
	MEMBER_OF_SAMURDHI CHAR(1),
	SAMURDHI_ID VARCHAR2(10),
	SAMURDHI_SHARE_PRICE NUMBER(6 , 2),
	NO_OF_SHARES NUMBER(2 , 0),
	INTEREST_RATE NUMBER(4 , 2),
	YEARS NUMBER(2 , 0),
	INSTALLMENT NUMBER(7 , 2),
	TOTAL_INTEREST NUMBER(8 , 2),
	ADD_USER VARCHAR2(8),
	ADD_DATE DATE,
	ADD_TIME VARCHAR2(11),
	UPD_USER VARCHAR2(8),
	UPD_DATE DATE,
	UPD_TIME VARCHAR2(11)	);

CREATE UNIQUE INDEX PK_SPAPPLON ON SPAPPLON (ID_NO ASC, APPLICATION_NO ASC);

ALTER TABLE SPAPPLON ADD CONSTRAINT PK_SPAPPLON PRIMARY KEY (ID_NO, APPLICATION_NO);

ALTER TABLE SPAPPLON ADD CONSTRAINT FK_SPAPPLON_D FOREIGN KEY (DEPT_ID)	REFERENCES GLDEPTM (DEPT_ID)	ON DELETE RESTRICT	ON UPDATE CASCADE;

ALTER TABLE SPAPPLON ADD CONSTRAINT FK_SPAPPLON_C FOREIGN KEY (ID_NO)	REFERENCES APPLICANT (ID_NO)	ON DELETE RESTRICT	ON UPDATE CASCADE;

alter TABLE SPAPPLON ADD APPROVAL_DS char(1);
alter TABLE SPAPPLON ADD APPROVAL_SDO char(1);
alter TABLE SPAPPLON ADD APPROVAL_SBM char(1);

alter TABLE SPAPPLON drop (APPROVAL_SF)DO char(1);
alter TABLE SPAPPLON drop (APPROVAL_SM) char(1);

CREATE TABLE LETTER_REASONS (
		DEPT_ID CHAR(6) NOT NULL,
		LETTER_TYPE VARCHAR2(20) NOT NULL,
		REASON_CODE VARCHAR2(3) NOT NULL,
		REASON VARCHAR2(500)
	);

