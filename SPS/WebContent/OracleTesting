		no_of_phase SMALLINT,
		cust_type CHAR(2) NOT NULL,
		sec_deposit DECIMAL(10 , 2) NOT NULL,
		sm_charge DECIMAL(10 , 2) NOT NULL,
		dly_pack_no CHAR(2) NOT NULL,
		reader_code CHAR(2) NOT NULL,
		walk_seq CHAR(5) NOT NULL,
		no_of_metrs SMALLINT NOT NULL,
		init_rd_dt DATE NOT NULL,
		met_no1 CHAR(8),
		met_type1 CHAR(1),
