		DEPT_ID CHAR(6) NOT NULL,
		FILE_NAME VARCHAR2(50),
		FILE_PATH VARCHAR2(100),
		FILE_SIZE NUMBER(6 , 0) DEFAULT 0  NOT NULL,
		NO_OF_JOBS NUMBER(4 , 0) DEFAULT 0  NOT NULL,
		EXPORTED_DATE DATE NOT NULL,
		EXPORTED_TIME VARCHAR2(11) NOT NULL,
		ACKNOWLEDGE CHAR(1)
	);

