CREATE TABLE SPESTCND (
		CONTRACTOR_ID VARCHAR2(4) NOT NULL,
		PROJECT_NO CHAR(20) NOT NULL,
		DEPT_ID CHAR(6) NOT NULL,
		CONSUMER_NAME VARCHAR2(60),
		AMOUNT NUMBER(10 , 2),
		SEAL_NO VARCHAR2(10),
		ALLOCATED_USER VARCHAR2(8),
		ALLOCATED_DATE DATE,
		ALLOCATED_TIME VARCHAR2(11),
		FINISHED_USER VARCHAR2(8),
		FINISHED_DATE DATE,
		FINISHED_TIME VARCHAR2(11),
		STATUS VARCHAR2(2),
		IS_EXPORTED CHAR(1)
	);

CREATE UNIQUE INDEX PK_SPESTCND ON SPESTCND (CONTRACTOR_ID ASC, PROJECT_NO ASC, DEPT_ID ASC);

CREATE UNIQUE INDEX SYS_C0021366 ON SPESTCND (PROJECT_NO ASC);

