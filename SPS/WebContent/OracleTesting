//APPLICATION_SUB_TYPE VARCHAR2(2) must be not null change later
CREATE TABLE APPLICATIONS (
		APPLICATION_ID VARCHAR2(20) NOT NULL,
		DEPT_ID CHAR(6) NOT NULL,
		APPLICATION_NO VARCHAR2(21),
		APPLICATION_TYPE VARCHAR2(2) NOT NULL,
		APPLICATION_SUB_TYPE VARCHAR2(2), 		
		SUBMIT_DATE DATE DEFAULT SYSDATE  NOT NULL,
		ID_NO VARCHAR2(10) NOT NULL,
		PREPARED_BY VARCHAR2(10) NOT NULL,
		CONFIRMED_BY VARCHAR2(10),
		CONFIRMED_DATE DATE,
		CONFIRMED_TIME VARCHAR2(11),
		ALLOCATED_TO VARCHAR2(10),
		ALLOCATED_BY VARCHAR2(10),
		ALLOCATED_DATE DATE,
		ALLOCATED_TIME VARCHAR2(11),
		STATUS VARCHAR2(2) NOT NULL,
		ADD_USER VARCHAR2(8),
		ADD_DATE DATE,
		ADD_TIME VARCHAR2(11),
		UPD_USER VARCHAR2(8),
		UPD_DATE DATE,
		UPD_TIME VARCHAR2(11),
		DESCRIPTION VARCHAR2(500),
		FROM_DATE DATE,
		TO_DATE DATE,
		DURATION_IN_DAYS NUMBER(4 , 0),
		DURATION_TYPE CHAR(1),
		DURATION NUMBER(4 , 2)
		
	);

CREATE UNIQUE INDEX SYS_C0016411 ON APPLICATIONS (APPLICATION_NO ASC);

CREATE UNIQUE INDEX PK_APPLICATIONS ON APPLICATIONS (APPLICATION_ID ASC, DEPT_ID ASC);

ALTER TABLE APPLICATIONS ADD CONSTRAINT PK_APPLICATIONS PRIMARY KEY (APPLICATION_ID, DEPT_ID);

ALTER TABLE APPLICATIONS ADD CONSTRAINT FK_APPLICATIONS_ID_NO FOREIGN KEY (ID_NO)	REFERENCES APPLICANT (ID_NO)	ON DELETE RESTRICT	ON UPDATE CASCADE;

ALTER TABLE APPLICATIONS ADD CONSTRAINT FK_APPLICATIONS_DEPT_ID FOREIGN KEY (DEPT_ID)	REFERENCES GLDEPTM (DEPT_ID)	ON DELETE RESTRICT	ON UPDATE CASCADE;

ALTER TABLE APPLICATIONS ADD disconnected_Within NUMBER(3)
ALTER TABLE APPLICATIONS ADD finalized_Within NUMBER(3)

ALTER TABLE APPLICATIONS ADD (IS_LOAN_APP CHAR(1))
ALTER TABLE APPLICATIONS drop (ISLOANAPP) CHAR(1))
select * from APPLICATIONS


//drop table WIRING_LAND_DETAIL 
CREATE TABLE WIRING_LAND_DETAIL (

		APPLICATION_ID VARCHAR2(21) NOT NULL,
		DEPT_ID CHAR(6) NOT NULL,
		SERVICE_STREET_ADDRESS VARCHAR2(30) NOT NULL,
		SERVICE_SUBURB VARCHAR2(30) NOT NULL,
		SERVICE_CITY VARCHAR2(30) NOT NULL,
		SERVICE_POSTAL_CODE NUMBER(5 , 0),
		OWNERSHIP CHAR(1) NOT NULL,
		OCCUPY_OWNER_CERTIFIED CHAR(1) NOT NULL,
		IS_GOVERNMENT_PLACE CHAR(1) NOT NULL,
		LAND_DISPUTE CHAR(1) NOT NULL,
		ASSESSMENT_NO VARCHAR2(6) NOT NULL,
		NEIGHBOURS_ACC_NO VARCHAR2(10),
		NO_OF_BULBS NUMBER(3 , 0) ,
		NO_OF_FANS NUMBER (3 , 0) ,
		NO_OF_PLUGS_5A NUMBER(3 , 0) ,
		NO_OF_PLUGS_15A NUMBER(3 , 0) ,
		MOTOR_TOTAL NUMBER(3 , 0) ,
		WELDING_PLANT NUMBER(3 , 0) ,
		METAL_CRUSHER NUMBER(3 , 0) ,
		SAW_MILLS NUMBER(3 , 0),
		PHASE NUMBER(1 , 0) NOT NULL,
		CONNECTION_TYPE NUMBER(2 , 0) NOT NULL,
		TARIFF_CAT_CODE VARCHAR2(2) NOT NULL,
		TARIFF_CODE VARCHAR2(2) NOT NULL,
		CUSTOMER_CATEGORY VARCHAR2(4) NOT NULL,
		CUSTOMER_TYPE VARCHAR2(4) NOT NULL,
		IS_LOOP_SERVICE CHAR(1) NOT NULL

	);

CREATE UNIQUE INDEX PK_WIRING_LAND_DETAIL ON WIRING_LAND_DETAIL (APPLICATION_ID ASC, DEPT_ID ASC);

ALTER TABLE WIRING_LAND_DETAIL ADD CONSTRAINT PK_WIRING_LAND_DETAIL PRIMARY KEY (APPLICATION_ID, DEPT_ID);

ALTER TABLE WIRING_LAND_DETAIL ADD CONSTRAINT FK_WIRING_LAND_DETAIL_T FOREIGN KEY (TARIFF_CODE) REFERENCES TARIFF (TARIFF_CODE) ON DELETE RESTRICT ON UPDATE CASCADE;

ALTER TABLE WIRING_LAND_DETAIL ADD CONSTRAINT FK_WIRING_LAND_DETAIL FOREIGN KEY (APPLICATION_ID, DEPT_ID) REFERENCES APPLICATIONS (APPLICATION_ID, DEPT_ID) ON DELETE RESTRICT 	ON UPDATE CASCADE;

ALTER TABLE WIRING_LAND_DETAIL Drop (IS_LOOP_SERVICE)
ALTER TABLE WIRING_LAND_DETAIL Drop (LAND_DISPUTE)

ALTER TABLE WIRING_LAND_DETAIL Modify (ASSESSMENT_NO VARCHAR2(6) null)

CREATE TABLE APPLICATION_REFERENCE (
	APPLICATION_ID VARCHAR2(21) NOT NULL,
	DEPT_ID CHAR(6) NOT NULL,
	APPLICATION_NO VARCHAR2(21) NOT NULL,
	STATUS VARCHAR2(2),
	ID_NO VARCHAR2(10),
	PROJECTNO VARCHAR2(21),
	CONSRUCTOR_ID VARCHAR2(4),
	POSTED_BY VARCHAR2(8),
	POSTED_DATE DATE,
	POSTED_TIME VARCHAR2(11)
	
	);

CREATE UNIQUE INDEX SYS_C0016418 ON APPLICATION_REFERENCE (APPLICATION_NO ASC);

CREATE UNIQUE INDEX PK_APP_REF ON APPLICATION_REFERENCE (APPLICATION_ID ASC, DEPT_ID ASC);

ALTER TABLE APPLICATION_REFERENCE ADD CONSTRAINT PK_APP_REF PRIMARY KEY (APPLICATION_ID, DEPT_ID);

ALTER TABLE APPLICATION_REFERENCE ADD CONSTRAINT FK_APP_REF FOREIGN KEY (APPLICATION_ID, DEPT_ID)	REFERENCES APPLICATIONS (APPLICATION_ID, DEPT_ID)	ON DELETE RESTRICT	ON UPDATE CASCADE;

CREATE TABLE PIV_DETAIL (	
	PIV_NO VARCHAR2(21) NOT NULL,	
	PIV_SEQ_NO NUMBER(2 , 0) NOT NULL,
	DEPT_ID CHAR(6) NOT NULL,
	PIV_RECEIPT_NO VARCHAR2(10),
	REFERENCE_TYPE CHAR(3) NOT NULL,	
	REFERENCE_NO VARCHAR2(21) NOT NULL,	
	APP_REFERENCE_NO VARCHAR2(21),	
	EST_REFERENCE_NO VARCHAR2(21),	
	REVISE_REFERENCE_NO VARCHAR2(21),
	ID_NO VARCHAR2(10) NOT NULL,	
	PIV_DATE DATE DEFAULT SYSDATE  NOT NULL,	
	PIV_AMOUNT NUMBER(10 , 2) NOT NULL,	
	CEB_BRANCH_NAME VARCHAR2(20),	
	BANK_CODE VARCHAR2(4),	
	BRANCH_CODE VARCHAR2(4),	
	PAYMENT_MODE CHAR(1),	
	BANK_PAID_DATE DATE,	
	PAID_DATE DATE,	
	CHEQUE_BANK_CODE VARCHAR2(4),	
	CHEQUE_BRANCH_CODE VARCHAR2(4),	
	CHEQUE_DATE DATE,	
	CHEQUE_NO NUMBER(8 , 0),	
	MISCELLANEOUS_INCOME NUMBER(10 , 2) DEFAULT 0.00  NOT NULL,	
	ELECTRICITY_DEBTORS NUMBER(10 , 2) DEFAULT 0.00  NOT NULL,	
	SECURITY_DEPOSIT NUMBER(10 , 2) DEFAULT 0.00  NOT NULL,		
	SER_CONN_OR_ELEC_SCH NUMBER(10 , 2) DEFAULT 0.00  NOT NULL,	
	TENDER_DEPOSIT NUMBER(10 , 2) DEFAULT 0.00  NOT NULL,	
	MISCELLANEOUS_DEPOSIT NUMBER(10 , 2) DEFAULT 0.00  NOT NULL,	
	CASH_IN_TRANSIT NUMBER(10 , 2) DEFAULT 0.00  NOT NULL,	
	FOR_DISHONORED_CHEQUE NUMBER(10 , 2) DEFAULT 0.00  NOT NULL,	
	SUB_TOTAL NUMBER(10 , 2) DEFAULT 0.00  NOT NULL,	
	VAT NUMBER(10 , 2) DEFAULT 0.00  NOT NULL,	
	GRAND_TOTAL NUMBER(10 , 2) DEFAULT 0.00  NOT NULL,	
	LOAN_REFERENCE VARCHAR2(10),	
	LOAN_AMOUNT NUMBER(10 , 2),	
	PREPARED_BY VARCHAR2(10) NOT NULL,	
	APPROVED_BY VARCHAR2(10),	
	CONFIRMED_BY VARCHAR2(10),	
	CONFIRMED_DATE DATE,	
	CONFIRMED_TIME VARCHAR2(11),	
	STATUS VARCHAR2(2) NOT NULL,	
	ADD_USER VARCHAR2(8),	
	ADD_DATE DATE,	
	ADD_TIME VARCHAR2(11),	
	UPD_USER VARCHAR2(8),	
	UPD_DATE DATE,	
	UPD_TIME VARCHAR2(11),	
	CANCEL_USER VARCHAR2(8),	
	CANCEL_DATE DATE,	
	CANCEL_TIME VARCHAR2(11),	
	DESCRIPTION VARCHAR2(500)	
	

	);

CREATE UNIQUE INDEX PK_PIV_DETAIL ON PIV_DETAIL (PIV_NO ASC, DEPT_ID ASC);

ALTER TABLE PIV_DETAIL ADD CONSTRAINT PK_PIV_DETAIL PRIMARY KEY (PIV_NO, DEPT_ID);

ALTER TABLE PIV_DETAIL ADD CONSTRAINT FK_PIV_DETAIL_ID_NO FOREIGN KEY (ID_NO)	REFERENCES APPLICANT (ID_NO)	ON DELETE RESTRICT	ON UPDATE CASCADE;

ALTER TABLE PIV_DETAIL ADD CONSTRAINT FK_PIV_CHQ_BANK FOREIGN KEY (CHEQUE_BRANCH_CODE, CHEQUE_BANK_CODE)	REFERENCES BANK_BRANCH (BRANCH_CODE, BANK_CODE)	ON DELETE RESTRICT	ON UPDATE CASCADE;

ALTER TABLE PIV_DETAIL ADD CONSTRAINT FK_PIV_DETAIL_DEPT_ID FOREIGN KEY (DEPT_ID)	REFERENCES GLDEPTM (DEPT_ID)	ON DELETE RESTRICT	ON UPDATE CASCADE;

ALTER TABLE PIV_DETAIL ADD CONSTRAINT FK_PIV_DETAIL_REF FOREIGN KEY (APP_REFERENCE_NO, DEPT_ID)	REFERENCES APPLICATIONS (APPLICATION_ID, DEPT_ID)	ON DELETE RESTRICT	ON UPDATE CASCADE;

ALTER TABLE PIV_DETAIL ADD CONSTRAINT FK_PIV_BANK FOREIGN KEY (BRANCH_CODE, BANK_CODE)	REFERENCES BANK_BRANCH (BRANCH_CODE, BANK_CODE)	ON DELETE RESTRICT	ON UPDATE CASCADE;

ALTER TABLE PIV_DETAIL MODIFY (CEB_BRANCH_NAME VARCHAR2(100)),	


ALTER TABLE PIV_DETAIL DROP CONSTRAINT FK_PIV_DETAIL_REF FOREIGN KEY (APP_REFERENCE_NO, DEPT_ID)	REFERENCES APPLICATIONS (APPLICATION_ID, DEPT_ID)	ON DELETE RESTRICT	ON UPDATE CASCADE;

ALTER TABLE PIV_DETAIL ADD POS_CENTER VARCHAR2(5)
ALTER TABLE PIV_DETAIL ADD POS_A VARCHAR2(5)
ALTER TABLE PIV_DETAIL modify(POS_CENTER VARCHAR2(6))

drop table BANK;
drop table BANK_BRANCH

CREATE TABLE BANK (
	BANK_CODE VARCHAR2(4) NOT NULL,
	BANK_NAME VARCHAR2(100)	);

CREATE UNIQUE INDEX PK_BANK ON BANK (BANK_CODE ASC);

ALTER TABLE BANK ADD CONSTRAINT PK_BANK PRIMARY KEY (BANK_CODE);

CREATE TABLE BANK_BRANCH (
	BRANCH_CODE VARCHAR2(3) NOT NULL,
	BANK_CODE VARCHAR2(4) NOT NULL,
	BRANCH_NAME VARCHAR2(100),
	BRANCH_ADDRESS VARCHAR2(200)
	);

CREATE UNIQUE INDEX PK_BANK_BRANCH ON BANK_BRANCH (BRANCH_CODE ASC, BANK_CODE ASC);

ALTER TABLE BANK_BRANCH ADD CONSTRAINT PK_BANK_BRANCH PRIMARY KEY (BRANCH_CODE, BANK_CODE);

ALTER TABLE BANK_BRANCH ADD CONSTRAINT FK_BANK_BRANCH FOREIGN KEY (BANK_CODE)	REFERENCES BANK (BANK_CODE)	ON DELETE RESTRICT	ON UPDATE CASCADE;

//DROP table  TARIFF

CREATE TABLE TARIFF (	
	TARIFF_CODE VARCHAR2(2) NOT NULL,	
	TARIFF_NAME VARCHAR2(40) NOT NULL,	
	TARIFF_CAT_CODE VARCHAR2(2) NOT NULL	);

CREATE UNIQUE INDEX PK_TARIFF ON TARIFF (TARIFF_CODE ASC);

ALTER TABLE TARIFF ADD CONSTRAINT PK_TARIFF PRIMARY KEY (TARIFF_CODE);

ALTER TABLE TARIFF ADD CONSTRAINT FK_TARIFF FOREIGN KEY (TARIFF_CAT_CODE)	REFERENCES TARIFF_CATEGORY (TARIFF_CAT_CODE)	ON DELETE RESTRICT	ON UPDATE CASCADE;

CREATE TABLE TARIFF_CATEGORY (	
	TARIFF_CAT_CODE VARCHAR2(2) NOT NULL,	
	TARIFF_CAT_NAME VARCHAR2(40) NOT NULL
	);

CREATE UNIQUE INDEX PK_TARIFF_CATEGORY ON TARIFF_CATEGORY (TARIFF_CAT_CODE ASC);

ALTER TABLE TARIFF_CATEGORY ADD CONSTRAINT PK_TARIFF_CATEGORY PRIMARY KEY (TARIFF_CAT_CODE);

