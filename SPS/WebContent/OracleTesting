drop TABLE SPESTSTD
CREATE TABLE SPESTSTD (
		ESTIMATE_NO VARCHAR2(20) NOT NULL,
		DEPT_ID CHAR(6) NOT NULL,
		SIN_NO VARCHAR2(5),
		TRANSFORM_COLOR CHAR(1),
		SERVICE_DISTANCE NUMBER(4 , 2) DEFAULT 0.00,
		LINE_LENGTH NUMBER(5 , 2) DEFAULT 0.00  ,
		LOOP_LENGTH NUMBER(5 , 2) DEFAULT 0.00  ,
		CONNECTOR_TYPE VARCHAR2(4) ,
		WIRING_TYPE CHAR(2),
		LB NUMBER(5 , 2) DEFAULT 0.00  ,
		SPAN NUMBER(1 , 0) DEFAULT 0  ,
		POLE_NO NUMBER(3 , 0) DEFAULT 0 ,
		POLE_TYPE VARCHAR2(6),
		POLE_SIZE VARCHAR2(6),
		STAY_NO NUMBER(3 , 0) DEFAULT 0  ,
		STRUT_NO NUMBER(3 , 0) DEFAULT 0  ,
		FIXED_COST NUMBER(10 , 2) DEFAULT 0.00  ,
		VARIABLE_COST NUMBER(10 , 2) DEFAULT 0.00 ,	
		SECURITY_DEPOSIT NUMBER(10 , 2) DEFAULT 0.00 ,	
		OTHER_COST NUMBER(10 , 2) DEFAULT 0.00  ,	
		TEMPORARY_DEPOSIT NUMBER(10 , 2) DEFAULT 0.00  ,	
		CONVERSION_COST NUMBER(10 , 2) DEFAULT 0.00  ,	
		LABOUR_COST NUMBER(10 , 2) DEFAULT 0.00  ,	
		TRANSPORT_COST NUMBER(10 , 2) DEFAULT 0.00  ,	
		OVERHEAD_COST NUMBER(10 , 2) DEFAULT 0.00  ,	
		ADDL_DEPOSIT NUMBER(10 , 2) DEFAULT 0.00  ,	
		DAMAGE_COST NUMBER(10 , 2) DEFAULT 0.00  ,	
		TOTAL_COST NUMBER(10 , 2) DEFAULT 0.00  ,	
		UPD_USER VARCHAR2(8),
		UPD_DATE DATE,
		UPD_TIME VARCHAR2(11),
		
		
	);

CREATE UNIQUE INDEX PK_SPESTSTD ON SPESTSTD (ESTIMATE_NO ASC, DEPT_ID ASC);

ALTER TABLE SPESTSTD ADD CONSTRAINT PK_SPESTSTD PRIMARY KEY (ESTIMATE_NO, DEPT_ID);

ALTER TABLE SPESTSTD ADD CONSTRAINT FK_SPESTSTD_D FOREIGN KEY (DEPT_ID) REFERENCES GLDEPTM (DEPT_ID) ON DELETE RESTRICT ON UPDATE CASCADE;

