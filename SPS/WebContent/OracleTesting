ALTER TABLE LETTER_REASONS ADD CONSTRAINT PK_LETTER_REASONS PRIMARY KEY (DEPT_ID, LETTER_TYPE, REASON_CODE);

ALTER TABLE LETTER_REASONS ADD CONSTRAINT FK_LETTER_REASONS FOREIGN KEY (DEPT_ID) REFERENCES GLDEPTM (DEPT_ID)

	ON DELETE RESTRICT

	ON UPDATE CASCADE;
	
	
