ALTER TABLE SPESTPEM ADD CONSTRAINT FK_SPESTPEM_D FOREIGN KEY (DEPT_ID) REFERENCES GLDEPTM (DEPT_ID) ON DELETE RESTRICT ON UPDATE CASCADE;


