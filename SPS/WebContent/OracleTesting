CREATE TABLE GLDEPTIN (
		DEPT_ID CHAR(6) NOT NULL,
		DEPT_TYPE VARCHAR2(10),
		DEPT_AREA VARCHAR2(20),
		DEPT_PROVINCE VARCHAR2(20),
		DEPT_REGION VARCHAR2(20),
		DEPT_FULL_NAME VARCHAR2(50),
		DEPT_ADD VARCHAR2(200),
		DEPT_TEL VARCHAR2(10),
		BULK_SUPPLIER_NAME VARCHAR2(100),
		BULK_SUPPLIER_ADD VARCHAR2(200),
		BULK_SUPPLIER_TEL NUMBER(10 , 0),
		BRANCH_CODE VARCHAR2(3),
		BANK_CODE VARCHAR2(4)
		
	);

CREATE UNIQUE INDEX PK_GLDEPTINF ON GLDEPTIN (DEPT_ID ASC);

ALTER TABLE GLDEPTIN ADD CONSTRAINT PK_GLDEPTINF PRIMARY KEY (DEPT_ID);

ALTER TABLE GLDEPTIN ADD CONSTRAINT FK_GLDEPTINF_D FOREIGN KEY (DEPT_ID) REFERENCES GLDEPTM (DEPT_ID) ON DELETE RESTRICT ON UPDATE CASCADE;

ALTER TABLE GLDEPTIN ADD CONSTRAINT FK_GLDEPTINF_B FOREIGN KEY (BRANCH_CODE, BANK_CODE) REFERENCES BANK_BRANCH (BRANCH_CODE, BANK_CODE) ON DELETE RESTRICT ON UPDATE CASCADE;

