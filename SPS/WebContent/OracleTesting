ALTER TABLE GLDEPTIN ADD CONSTRAINT FK_GLDEPTINF_D FOREIGN KEY (DEPT_ID)

	REFERENCES GLDEPTM (DEPT_ID)

	ON DELETE RESTRICT

	ON UPDATE CASCADE;

