CREATE TABLE APPLICANT (

		ID_NO VARCHAR2(10) NOT NULL,

		ID_TYPE VARCHAR2(3) NOT NULL,

		FIRST_NAME VARCHAR2(30) NOT NULL,

		LAST_NAME VARCHAR2(30) NOT NULL,

		STREET_ADDRESS VARCHAR2(30) NOT NULL,

		SUBURB VARCHAR2(30) NOT NULL,

		CITY VARCHAR2(30) NOT NULL,

		POSTAL_CODE VARCHAR2(5),

		EMAIL VARCHAR2(30),

		TELEPHONE_NO VARCHAR2(10),

		MOBILE_NO VARCHAR2(10),

		CEB_EMPLOYEE CHAR(1),

		PREFERRED_LANGUAGE VARCHAR2(2) NOT NULL,

		STATUS VARCHAR2(1),

		ADD_USER VARCHAR2(8),

		ADD_DATE DATE,

		ADD_TIME VARCHAR2(11),

		UPD_USER VARCHAR2(8),

		UPD_DATE DATE,

		UPD_TIME VARCHAR2(11),

		ENTITLED_4_LOAN CHAR(1),

		MEMBER_OF_SAMURDHI CHAR(1),

		SAMURDHI_ID VARCHAR2(10),

		SHARE_PRICE NUMBER(6 , 2),

		NO_OF_SHARES NUMBER(2 , 0),

		LOAN_REFERENCE VARCHAR2(10),

		LOAN_AMOUNT NUMBER(10 , 2),

		COMPANY_NAME VARCHAR2(100),

		DEPT_ID CHAR(6),

		FULL_NAME VARCHAR2(100)

	);

CREATE UNIQUE INDEX PK_APPLICANT ON APPLICANT (ID_NO ASC);

ALTER TABLE APPLICANT ADD CONSTRAINT PK_APPLICANT PRIMARY KEY (ID_NO);

ALTER TABLE APPLICANT ADD CONSTRAINT FK_APPLICANT_ID FOREIGN KEY (DEPT_ID)

	REFERENCES GLDEPTM (DEPT_ID)

	ON DELETE RESTRICT

	ON UPDATE CASCADE;


CREATE TABLE APPLICATION_REFERENCE (

		APPLICATION_ID VARCHAR2(21) NOT NULL,

		DEPT_ID CHAR(6) NOT NULL,

		APPLICATION_NO VARCHAR2(21) NOT NULL,

		STATUS VARCHAR2(2),

		ID_NO VARCHAR2(10),

		PROJECTNO VARCHAR2(21),

		CONSRUCTOR_ID VARCHAR2(4),

		POSTED_BY VARCHAR2(8),

		POSTED_DATE DATE,

		POSTED_TIME VARCHAR2(11)

	);

CREATE UNIQUE INDEX PK_APP_REF ON APPLICATION_REFERENCE (APPLICATION_ID ASC, DEPT_ID ASC);

CREATE UNIQUE INDEX SYS_C0016418 ON APPLICATION_REFERENCE (APPLICATION_NO ASC);

ALTER TABLE APPLICATION_REFERENCE ADD CONSTRAINT PK_APP_REF PRIMARY KEY (APPLICATION_ID, DEPT_ID);

ALTER TABLE APPLICATION_REFERENCE ADD CONSTRAINT FK_APP_REF FOREIGN KEY (APPLICATION_ID, DEPT_ID)

	REFERENCES APPLICATIONS (APPLICATION_ID, DEPT_ID)

	ON DELETE RESTRICT

	ON UPDATE CASCADE;


CREATE TABLE APPLICATIONS (

		APPLICATION_ID VARCHAR2(20) NOT NULL,

		DEPT_ID CHAR(6) NOT NULL,

		APPLICATION_NO VARCHAR2(21),

		APPLICATION_TYPE VARCHAR2(2) NOT NULL,

		APPLICATION_SUB_TYPE VARCHAR2(2),

		SUBMIT_DATE DATE DEFAULT SYSDATE  NOT NULL,

		ID_NO VARCHAR2(10) NOT NULL,

		PREPARED_BY VARCHAR2(10) NOT NULL,

		CONFIRMED_BY VARCHAR2(10),

		CONFIRMED_DATE DATE,

		CONFIRMED_TIME VARCHAR2(11),

		ALLOCATED_TO VARCHAR2(10),

		ALLOCATED_BY VARCHAR2(10),

		ALLOCATED_DATE DATE,

		ALLOCATED_TIME VARCHAR2(11),

		STATUS VARCHAR2(2) NOT NULL,

		ADD_USER VARCHAR2(8),

		ADD_DATE DATE,

		ADD_TIME VARCHAR2(11),

		UPD_USER VARCHAR2(8),

		UPD_DATE DATE,

		UPD_TIME VARCHAR2(11),

		DESCRIPTION VARCHAR2(500),

		FROM_DATE DATE,

		TO_DATE DATE,

		DURATION_IN_DAYS NUMBER(4 , 0),

		DURATION_TYPE CHAR(1),

		DURATION NUMBER(4 , 2),

		DISCONNECTED_WITHIN NUMBER(3 , 0),

		FINALIZED_WITHIN NUMBER(3 , 0),

		IS_LOAN_APP CHAR(1)

	);

CREATE UNIQUE INDEX PK_APPLICATIONS ON APPLICATIONS (APPLICATION_ID ASC, DEPT_ID ASC);

CREATE UNIQUE INDEX SYS_C0016411 ON APPLICATIONS (APPLICATION_NO ASC);

ALTER TABLE APPLICATIONS ADD CONSTRAINT PK_APPLICATIONS PRIMARY KEY (APPLICATION_ID, DEPT_ID);

ALTER TABLE APPLICATIONS ADD CONSTRAINT FK_APPLICATIONS_ID_NO FOREIGN KEY (ID_NO)

	REFERENCES APPLICANT (ID_NO)

	ON DELETE RESTRICT

	ON UPDATE CASCADE;

ALTER TABLE APPLICATIONS ADD CONSTRAINT FK_APPLICATIONS_DEPT_ID FOREIGN KEY (DEPT_ID)

	REFERENCES GLDEPTM (DEPT_ID)

	ON DELETE RESTRICT

	ON UPDATE CASCADE;


