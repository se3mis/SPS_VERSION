drop table SPCSTNGM

CREATE TABLE SPCSTNGM (

		PHASE NUMBER(1 , 0) NOT NULL,
		CONNECTION_TYPE NUMBER(2 , 0) NOT NULL,
		TARIFF_CAT_CODE VARCHAR2(2) NOT NULL,
		FROM_LENGTH NUMBER(4,0) NOT NULL,
		TO_LENGTH NUMBER(4,0) NOT NULL,
		DEPT_ID CHAR(6) NOT NULL,
		FIXED_COST NUMBER(10 , 2),
		SECURITY_DEPOSIT NUMBER(10 , 2),
		TEMPORARY_DEPOSIT NUMBER(10 , 2),
		WIRE_METER_PRICE NUMBER(7 , 2),
		UPD_USER VARCHAR2(8),
		UPD_DATE DATE,
		UPD_TIME VARCHAR2(11)
		

	);

CREATE UNIQUE INDEX PK_SPCSTNGM ON SPCSTNGM (PHASE ASC, CONNECTION_TYPE ASC, TARIFF_CAT_CODE ASC, DEPT_ID ASC, FROM_LENGTH  ASC, TO_LENGTH  ASC);

ALTER TABLE SPCSTNGM ADD CONSTRAINT PK_SPCSTNGM PRIMARY KEY (PHASE, CONNECTION_TYPE, TARIFF_CAT_CODE, DEPT_ID, FROM_LENGTH, TO_LENGTH);

ALTER TABLE SPCSTNGM ADD CONSTRAINT FK_SPCSTNGM FOREIGN KEY (DEPT_ID) REFERENCES GLDEPTM (DEPT_ID);

	ON DELETE RESTRICT

	ON UPDATE CASCADE;

ALTER TABLE SPCSTNGM ADD FROM_LENGTH NUMBER(4,0)
ALTER TABLE SPCSTNGM ADD TO_LENGTH NUMBER(4,0)


ALTER TABLE SPCSTNGM DROP CONSTRAINT PK_SPCSTNGM PRIMARY KEY (PHASE, CONNECTION_TYPE, TARIFF_CAT_CODE, DEPT_ID);






